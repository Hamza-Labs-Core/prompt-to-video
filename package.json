{
  "name": "prompt-to-video",
  "version": "1.0.0",
  "description": "AI-powered video generation pipeline using Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "dev:ui": "vite --config vite.config.ts",
    "deploy": "wrangler deploy",
    "build": "npm run build:ui && wrangler deploy --dry-run",
    "build:ui": "vite build --config vite.config.ts",
    "typecheck": "tsc --noEmit",
    "tail": "wrangler tail",
    "cf-typegen": "wrangler types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hamza-Labs-Core/prompt-to-video.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "video",
    "fal.ai",
    "creatomate"
  ],
  "author": "Hamza Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hamza-Labs-Core/prompt-to-video/issues"
  },
  "homepage": "https://github.com/Hamza-Labs-Core/prompt-to-video#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7",
    "@tanstack/react-query": "^5.14.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.53.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  }
}
